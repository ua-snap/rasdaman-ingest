        </HTTPSamplerProxy>
        <hashTree>
          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Jitter lat/lon (Groovy)" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">
import java.util.concurrent.ThreadLocalRandom
import java.util.Locale

double baseLat = Double.parseDouble(vars.get(&apos;baseLat&apos;))
double baseLon = Double.parseDouble(vars.get(&apos;baseLon&apos;))
double jitter  = Double.parseDouble(vars.get(&apos;jitterDeg&apos;))

def rnd = ThreadLocalRandom.current()
double lat = baseLat + rnd.nextDouble(-jitter, jitter)
double lon = baseLon + rnd.nextDouble(-jitter, jitter)

// clamp to valid ranges, just in case
lat = Math.max(-90.0, Math.min(90.0, lat))
if (lon &gt; 180.0) { lon -= 360.0 }
if (lon &lt; -180.0) { lon += 360.0 }

// format with &apos;.&apos; decimal separator
vars.put(&apos;lat&apos;, String.format(Locale.US, &apos;%.6f&apos;, lat))
vars.put(&apos;lon&apos;, String.format(Locale.US, &apos;%.6f&apos;, lon))
</stringProp>
