{
 "config": {
    "service_url": "http://localhost:8080/rasdaman/ows",
    "tmp_directory": "/tmp/",
    "crs_resolver": "http://localhost:8080/rasdaman/def/",
    "default_crs": "http://localhost:8080/rasdaman/def/crs/EPSG/0/3572",
    "mock": false,
    "automated": false
  },
  "input": {
    "coverage_id": "hsia_arctic_production",
    "paths": [
      "/tmp/hsia_updates_2021/rasdaman_hsia_arctic_production_tifs/*.tif"
    ]
  },
  "recipe": {
    "name": "time_series_irregular",
    "options": {
      "time_parameter": {
        "filename": {
          "regex": "seaice_conc_sic_mean_pct_monthly_panarctic_(.*)",
          "group": "1"
        },
        "datetime_format": "YYYY_MM"
      },
      "time_crs": "http://opengis.net/def/crs/OGC/0/AnsiDate",
      "wms_import": true
    }
  }
}
