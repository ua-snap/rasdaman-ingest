      "when": "after_import",
      "cmd": ". /etc/default/rasdaman; curl --user \"$RASCURL\" \"https://apollo.snap.uaf.edu/rasdaman/admin/layer/style/add?COVERAGEID=cmip6_monthly_e3sm&STYLEID=ardac_clt&ABSTRACT=Cloud%20Area%20Fraction%20(ARDAC)&WCPSQUERYFRAGMENT=%24c%5Bvarname%280%29%5D&COLORTABLETYPE=ColorMap&\" --data-urlencode \"COLORTABLEDEFINITION={\\\"type\\\":\\\"intervals\\\",\\\"colorTable\\\":{\\\"0\\\":[80,130,165,255],\\\"60\\\":[103,164,199,255],\\\"70\\\":[148,185,212,255],\\\"80\\\":[199,207,224,255],\\\"90\\\":[235,233,240,255],\\\"9999\\\":[0,0,0,0]}}\"",
