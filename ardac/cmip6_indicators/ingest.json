{
  "config": {
    "service_url": "https://localhost/rasdaman/ows",
    "tmp_directory": "/tmp/",
    "mock": false,
    "automated": true
  },
  "input": {
    "coverage_id": "cmip6_indicators_cf_wms",
    "paths": [
      "/opt/rasdaman-storage/coverage_data/cmip6_indicators/cmip6_indicators_ensemble.nc"
    ]
  },
  "recipe": {
    "name": "general_coverage",
    "options": {
      "wms_import": true,
      "import_order": "ascending",
      "tiling": "ALIGNED [0:*, 0:*, 0:*, 0:*, 0:*] tile size 4194304",
      "coverage": {
        "crs": "OGC/0/Index1D?axis-label=\"model\"@OGC/0/Index1D?axis-label=\"scenario\"@OGC/0/Index1D?axis-label=\"time\"@EPSG/0/4326",
        "metadata": {
          "type": "xml",
          "global": "auto"
        },
        "slicer": {
          "type": "netcdf",
          "pixelIsPoint": true,
          "bands": [
            {
              "name": "cd",
              "identifier": "cd",
              "nilValue": "nan"
            },
            {
              "name": "cdd",
              "identifier": "cdd",
              "nilValue": "-9999"
            },
            {
              "name": "csdi",
              "identifier": "csdi",
              "nilValue": "-9999"
            },
            {
              "name": "cwd",
              "identifier": "cwd",
              "nilValue": "-9999"
            },
            {
              "name": "dw",
              "identifier": "dw",
              "nilValue": "-9999"
            },
            {
              "name": "ftc",
              "identifier": "ftc",
              "nilValue": "-9999"
            },
            {
              "name": "hd",
              "identifier": "hd",
              "nilValue": "nan"
            },
            {
              "name": "r10mm",
              "identifier": "r10mm",
              "nilValue": "-9999"
            },
            {
              "name": "rx1day",
              "identifier": "rx1day",
              "nilValue": "-9999"
            },
            {
              "name": "rx5day",
              "identifier": "rx5day",
              "nilValue": "-9999"
            },
            {
              "name": "su",
              "identifier": "su",
              "nilValue": "-9999"
            },
            {
              "name": "wsdi",
              "identifier": "wsdi",
              "nilValue": "-9999"
            }
          ],
          "axes": {
            "model": {
              "min": "${netcdf:variable:model:min}",
              "max": "${netcdf:variable:model:max}",
              "directPositions": "${netcdf:variable:model}",
              "irregular": true,
              "gridOrder": 0
            },
            "scenario": {
              "min": "${netcdf:variable:scenario:min}",
              "max": "${netcdf:variable:scenario:max}",
              "directPositions": "${netcdf:variable:scenario}",
              "irregular": true,
              "gridOrder": 1
            },
            "time": {
              "min": "${netcdf:variable:time:min}",
              "max": "${netcdf:variable:time:max}",
              "directPositions": "${netcdf:variable:time}",
              "irregular": true,
              "gridOrder": 2
            },
            "lon": {
              "min": "${netcdf:variable:lon:min}",
              "max": "${netcdf:variable:lon:max}",
              "resolution": "${netcdf:variable:lon:resolution}",
              "gridOrder": 3
            },
            "lat": {
              "min": "${netcdf:variable:lat:min}",
              "max": "${netcdf:variable:lat:max}",
              "resolution": "${netcdf:variable:lat:resolution}",
              "gridOrder": 4
            }
          }
        }
      }
    }
  },
  "hooks" : [
